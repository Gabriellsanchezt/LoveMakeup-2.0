{
    "name": "lovemakeup/proyecto",
    "description": "PLATAFORMA DE COMERCIO ELECTRONICO PARA LA LÍNEA DE PRODUCTOS DE BELLEZA “LOVEMAKEUP” C.A.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "LoveMakeup\\Proyecto\\Modelo\\": "modelo/",
            "LoveMakeup\\Proyecto\\Controlador\\": "controlador/",
            "LoveMakeup\\Proyecto\\Config\\": "config/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Equipo de proyecto de LoveMakeup C.A.",
            "email": "help.lovemakeupca@gmail.com"
        }
    ],
    "require": {
        "dompdf/dompdf": "^2.0",
        "phpmailer/phpmailer": "^6.9"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "php-webdriver/webdriver": "^1.15",
        "symfony/process": "^6.0"
    },
    "scripts": {
        "test": "phpunit",
        "test-phpunit": "phpunit --testsuite=\"PHPUnit Tests\"",
        "test-selenium": "phpunit --testsuite=\"Selenium Tests\"",
        "test-all": "phpunit"
    }
}
